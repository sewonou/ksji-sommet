        <script src="{{ asset('js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('js/popper.min.js') }}"></script>
        <script src="{{ asset('test/lib/easing/easing.min.js') }}"></script>
        <script src="{{ asset('test/lib/superfish/hoverIntent.js') }}"></script>
        <script src="{{ asset('test/lib/superfish/superfish.min.js') }}"></script>
        <script src="{{ asset('test/lib/wow/wow.min.js') }}"></script>
        <script src="{{ asset('test/lib/venobox/venobox.min.js') }}"></script>
        <script src="{{ asset('test/lib/owlcarousel/owl.carousel.min.js') }}"></script>

        <!-- Contact Form JavaScript File -->
        <script src="contactform/contactform.js"></script>

        {% block javascripts %}{% endblock %}
        <!-- Template Main Javascript File -->
        <script src="{{ asset('test/js/main.js') }}"></script>
    </body>

